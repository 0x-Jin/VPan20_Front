.point-class{
  width: 7.1rem;
  height: 6.5rem;
  position: absolute;
  visibility: visible;
  display: block;
  top: -2rem;
}
.app-index-header{
  background:-webkit-gradient(linear, 0 0, 100% 0, from(#f2af0d), to(#fba331));
}
.pull-left{
  float: left;
}
.pull-right{
  float: right;
}
.center{
  text-align: center;
}
.red{
  color: #ff0000;
}
.green{
  color: #00ff00;
}

.phoneNumError-tip{
  position: absolute;
  z-index: 200;
  width: 40%;
  left: 10%;
  margin-top: 0.5rem;
  border-radius: 0.5rem;
  background-color: #21275b;
  color: #ff0000;
  text-align: center;
  padding: 2%;
}
.phoneNumError-tip .triangle-up {
  width: 0;
  height: 0;
  border-left: 1rem solid transparent;
  border-right: 1rem solid transparent;
  border-bottom: 1rem solid #21275b;
  position: absolute;
  top: -0.5rem;
  left: 0.8rem;
}

.pop-model-body{
