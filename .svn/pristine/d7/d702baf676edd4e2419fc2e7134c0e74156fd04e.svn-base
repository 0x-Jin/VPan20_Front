<ion-view view-title="" class="accountIndex-view">

  <ion-nav-bar class="app-index-header accountIndex-header">

    <ion-nav-buttons side="left">
      <a class="button button-icon icon ion-ios-arrow-left light" ng-click=""></a>
    </ion-nav-buttons>

    <ion-nav-buttons side="right">
      <span class="light">156xxxx2002</span>
      <a class="imgDiv" ng-href="#/tab/account-personalInfo">
        <img ng-src="img/temp/adam.jpg">
      </a>
    </ion-nav-buttons>

  </ion-nav-bar>

  <ion-content class="">

    <!--头部-->
    <div class="accountIndex-head">

      <!--昨日收益率-->
      <div>
        <br/>
        <p>昨日收益率</p>
        <p style="font-size: 45px; margin-top: 5%; margin-bottom: 7%">12%</p>
      </div>

      <!--账户余额、昨日收益率-->
      <div class="row" style="margin-top: 8%; margin-bottom: 3%;">

        <div class="col">
          <p>账户余额</p>
          <p class="number">2300.23</p>
        </div>
        <div class="col">
          <p>昨日收益率</p>
          <p class="number">2300.2</p>
        </div>

        <div style="clear: both"></div>

      </div>

      <!--充值、提现-->
      <div class="row rechargeAndWithdraw">

        <a class="col" style="border-right: 1px solid #fbdeb5;" ng-href="#/tab/account-recharge">
          <i class="icon ion-social-yen-outline"></i>
          <span>充值</span>
        </a>
        <a class="col" ng-href="#/tab/account-withdraw">
          <i class="icon ion-ios-bookmarks-outline"></i>
          <span>提现</span>
        </a>

      </div>

    </div>

    <div class="accountIndex-body">

      <div class="list">

        <a class="item" ng-href="#/tab/account-winTicket">
          <i class="icon ion-map"></i>
          我的赢家券
          <i class="icon ion-ios-arrow-right pull-right"></i>
        </a>

        <a class="item" ng-href="#/tab/account-broker">
          <i class="icon ion-clipboard"></i>
          经纪人
          <i class="icon ion-ios-arrow-right pull-right"></i>
        </a>

        <a class="item" ng-href="#/tab/account-history">
          <i class="icon ion-clock"></i>
          交易历史
          <i class="icon ion-ios-arrow-right pull-right"></i>
        </a>

        <a class="item" ng-href="#/tab/account-incomeAndExpenses">
          <i class="icon ion-social-yen-outline"></i>
          收支明细
          <i class="icon ion-ios-arrow-right pull-right"></i>
        </a>

        <a class="item" ng-href="#">
          <i class="icon ion-ios-analytics-outline"></i>
          交割订单
          <i class="icon ion-ios-arrow-right pull-right"></i>
        </a>

      </div>
    </div>


  </ion-content>
</ion-view>

