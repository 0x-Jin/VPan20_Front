  color: #ffffff
}
.accountIndex-view .accountIndex-head .rechargeAndWithdraw a > span{
  margin-left: 5%;
}
.accountIndex-view .accountIndex-body .list{
  margin-top: 3.5%;
}
.accountIndex-view .accountIndex-body .list .item{
  color: #ffffff;
  font-size: 15px;
  background-color: #04091e;
  padding: 3%;
  border: 1px solid rgba( 255,255,255,0.2);
}
.accountIndex-view .accountIndex-body .list .item .icon{
  margin: 0% 4%;
  font-size: 20px;
  color: rgba(255,255,255,0.7);
}

