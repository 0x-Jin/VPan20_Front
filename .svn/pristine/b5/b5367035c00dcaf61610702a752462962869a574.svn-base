/*充值*/
.account-recharge-view{
  background-color: #101322;
  color: #ffffff;
}
.account-recharge-view .priceBox{
  width: 124%;
  margin: 1% -4%;
}
.account-recharge-view .priceBox .box{
  width: 25%;
  text-align: center;
  padding: 6% 0%;
  margin: 2%;
  border: 1px solid rgba(255,255,255,0.2);
  float: left;
}
.account-recharge-view .priceBox .active{
  border: 1px solid #fcd230;
  position: relative;
}
.account-recharge-view .priceBox .active .triangle{
  width: 0;
  height: 0;
  border-top: 30px solid #fcd230;
  border-left: 30px solid transparent;
  position: absolute;
  right: 0px;
  top: 0px;
}
.account-recharge-view .priceBox .active > .triangle > .icon{
  color: #000000;
  font-size: 17px;
  position: absolute;
  right: 1px;
  top: -32px;
}
.account-recharge-view .smallAmmount{
  background-color: #101322;
  border: 1px solid rgba(255,255,255,0.5);
  text-align: center;
  margin-bottom: 0%;
}
.account-recharge-view .smallAmmount .row .col a{
  color: #ffffff;
}
.account-recharge-view .smallAmmount-option{
  background-color: #1b204b;
  margin-top: 0%;
}
.account-recharge-view .smallAmmount-option .list-con{
  display: none;
  overflow: auto;
  /*width: 93%;*/
  /*position: absolute;*/
}
.account-recharge-view .smallAmmount-option .list-con .list .item{
  text-align: center;
  background-color: #1b204b;
  color: #ffffff;
  padding: 2%;
  margin-bottom: 1px;
  border: none;
}
.account-recharge-view .smallAmmount-option .list-con .list .item-border-bottom{
  border-bottom: 1px solid rgba(255,255,255,0.2);
}
.account-recharge-view .rechargeBtn{
  width: 95%;
  margin: 0% 2.5%;
  font-size: 20px;
  background-color: #fcd230;
  border-radius: 0px;
  color: #ffffff;
}

