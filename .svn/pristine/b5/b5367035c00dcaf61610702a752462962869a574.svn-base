/* Empty. Add your own CSS if you like */
.point-class{
  width:71px;
  height: 65px;
  position: absolute;
  visibility: visible;
  display: block;
  top: -20px;
}
.app-index-header{
  background:-webkit-gradient(linear, 0 0, 100% 0, from(#f2af0d), to(#fba331));
}
.pull-left{
  float: left;
}
.pull-right{
  float: right;
}
.center{
  text-align: center;
}
.red{
  color: #ff0000;
}
.green{
  color: #00ff00;
}

.pop-model-body{
  padding: 0% 12%;
}

/*********马上交易**********/
.tradeIndex-view{
  background-color: #101322;
}
.tradeIndex-view .tradeIndex-head{
  background-color: #eea727;
  color: #ffffff;
  text-align: center;
}
.tradeIndex-view .tradeIndex-head > div img{
  width: 70px;
  height: 70px;
  margin-top: 5%;
  margin-bottom: 0.5%;
  border-radius: 70px;
}
.tradeIndex-view .tradeIndex-head > div.row .number{
  font-size: 20px;
  line-height: 5px;
}
.tradeIndex-view .tradeIndex-head .tradeIndex-head-btn{
  font-size: 16px;
  margin-top: 2%;
  text-decoration: none;
  color:#ffffff
}
.tradeIndex-view .tradeIndex-head .tradeIndex-head-btn span{
  margin-left: 4%;
}
.tradeIndex-view .tradeIndex-tabs{
  margin: 3% 0%;
  padding: 0%;
  color: #ffffff;
  text-align: center;
}
.tradeIndex-view .tradeIndex-tabs .row{
  margin: 0.5% 0%;
  padding: 0%;
}
.tradeIndex-view .tradeIndex-tabs .tabs-title .col{
  margin: 0% 0.5%;
  padding: 2.5% 0%;
  font-size: 20px;
  background-color: #29335c;
  border-radius: 3px;
}
.tradeIndex-view .tradeIndex-tabs .tabs-title .active{
  background-color: #d1720b;
}
.tradeIndex-view .tradeIndex-tabs .tabs-bottomBorder .col{
  margin: 0% 0.5%;
}
.tradeIndex-view .tradeIndex-tabs .tabs-bottomBorder .active{
  border-bottom: 1px solid #d1720b;
}
.tradeIndex-view .tradeIndex-currentPrice{
  color: #ffffff;
  text-align: center;
  font-size: 14px;
  line-height: 15px;
}
.tradeIndex-view .tradeIndex-currentPrice .latestPrice{
  font-size: 25px;
  padding: 0%;
  margin: 0%
}
.tradeIndex-view .tradeIndex-buy .productBox{
  text-align: center;
  color: #ffffff;
  margin:3%;
  margin-bottom: 0%;
}
.tradeIndex-view .tradeIndex-buy .productBox .buyUp{
  font-size: 16px;
  background-color: #f5583d;
  padding: 5% 0%;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
}
.tradeIndex-view .tradeIndex-buy .productBox .buyDown{
  font-size: 16px;
  background-color: #29be25;
  padding: 5% 0%;
  border-bottom-left-radius: 4px;
  border-bottom-right-radius: 4px;
}
/*买涨、买跌点击效果*/
.tradeIndex-view .tradeIndex-buy .productBox .buyUp:active{
  background-color: #c2c2c2;
}
.tradeIndex-view .tradeIndex-buy .productBox .buyDown:active{
  background-color: #c2c2c2;
}

.tradeIndex-view .tradeIndex-buy .productBox .productInfo{
  margin: 0%;
  padding-top: 8%;
  background-color: #15192b;
  border: 1px solid  rgba(255,255,255,0.3);
  border-top: 1px solid #15192b;
  border-bottom: 1px solid #15192b;
}
.tradeIndex-view .tradeIndex-notice{
  color: #ffffff;
  padding: 3% 2%;
  font-size: 13px;
  line-height: 20px;
  background-color: #121423;
}
.tradeIndex-view .tradeIndex-notice > div{
  position: relative; /*css的top属性指的是距最近的一个position属性为relative或者absolute的父级元素的上边的距离*/
  text-align: center;
  height: 20px;
  overflow-y: hidden;
}
.tradeIndex-view .tradeIndex-notice > div > div#noticeDivId{
  position: relative;
}
.tradeIndex-view .ECharts-tabs{
  color: #ffffff;
  border-top: 1px solid rgba(255,255,255,0.3);
  border-bottom: 1px solid rgba(255,255,255,0.3);
  padding: 0%;
  margin: 0%;
}
.tradeIndex-view .ECharts-tabs .col{
  margin: 0%;
  padding: 1% 0%;
  text-align: center;
  /*边框设为背景色。避免切换tabs时数据跳动*/
  border-left:  1px solid #101322;
  border-right:  1px solid #101322;
}
.tradeIndex-view .ECharts-tabs .active{
  background-color: #1f243b;
  border-left:  1px solid rgba(255,255,255,0.3);
  border-right:  1px solid rgba(255,255,255,0.3);
}

/************个人中心***********/
.accountIndex-view{
  background-color: #04091e;
}
.accountIndex-view .accountIndex-header .imgDiv{
