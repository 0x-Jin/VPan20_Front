  height: 200%;
  background-color: rgba(0,0,0,0.6);
  position: absolute;
  z-index: 100;
}
.instructionDiv{
  width: 100%;
  position: absolute;
  z-index: 101;
}
.instructionDiv img{
  position: absolute;
  width: 30%;
  right: 10px;
  top: 10px;
}
.instructionDiv > div{
  color: #ffffff;
  letter-spacing: 3px;
